
class N{
	char buf[100];
	int v;

	int N::operator+(N *n)
	{
		return (this->v - n->v);
	}

	int operatop-(N *n)
	{
		return (this-v - n->v)
	}
	
	void __thiscall N::setAnnotation(char *p)
	{
		size_t n;

		n = strlen(param_1);
		memcpy(this->buf, p, n);
		return;
	}
}

int main(int ac ,char **av)

{
  
  if (ac < 2) {
    exit(1);
  }
  N* n1 = new N(5);
  N* n2 = new N(6);
  n1->setAnnotation(av[1]));
  n1->operator+(n1);
  return (0);
}

